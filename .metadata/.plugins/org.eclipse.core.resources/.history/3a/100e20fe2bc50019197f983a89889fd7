#include <stdio.h>
#include <stdlib.h>
int Sumar(int* resultado);
int main(void)
{
	int resultado;
	printf("\nEl resultado es: %d",&resultado);
}
int Sumar(int* resultado)
{
	int numero1;
		int numero2;
		printf("\nNumero 1: ");
		scanf("%d",&numero1);
		printf("\nNumero 2: ");
		scanf("%d",&numero2);
		resultado=numero1+numero2;
		printf("resultado:%d",resultado);
		return 0;
	}
